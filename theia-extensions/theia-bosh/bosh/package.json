{
  "name": "@pivotal-tools/theia-bosh",
  "license": "EPL-1.0",
  "repository": "https://github.com/spring-projects/sts4",
  "description": "Bosh Deployment YAML Editor Support",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src",
    "jars"
  ],
  "dependencies": {
    "@theia/core": "latest",
    "@theia/languages": "latest",
    "@theia/monaco": "latest",
    "@pivotal-tools/theia-languageclient": "0.0.7"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/bosh-frontend-module",
      "backend": "lib/node/bosh-backend-module"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}